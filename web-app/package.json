{
  "name": "escherpad-web",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "core-decorators": "^0.20.0",
    "create-react-class": "^15.6.0",
    "express": "^4.15.3",
    "github-markdown-css": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "lato-font": "^3.0.0",
    "layout-components": "^2.7.1",
    "markdown-it": "^8.4.0",
    "morgan": "^1.8.2",
    "nextjs": "0.0.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-markdownit": "^2.5.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "save-dev": "^2.0.0",
    "styled-components": "^2.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-assets": "^0.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eywa-dropbox": "^1.12.0",
    "flow-bin": "^0.61.0",
    "flow-type": "^1.0.1",
    "flow-typed": "^2.1.2",
    "flow-watch": "^1.1.1",
    "luna-saga": "^3.1.1",
    "lz-string": "^1.4.4",
    "moment": "^2.19.3",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react-async-bootstrapper": "^1.1.2",
    "react-async-component": "^1.0.2",
    "react-json-pretty": "^1.6.3",
    "react-scripts": "^1.0.17",
    "react-styleguidist": "^5.5.4",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.5",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.4.0"
  },
  "scripts": {
    "start:browser-dev": "react-scripts start",
    "start:server-dev": "babel-watch ./server/index.js",
    "start:production": "NODE_ENV=production PUBLIC_DIR=../web-build node ./server-build/index.js",
    "start:styleguide": "styleguidist server",
    "build:web": "react-scripts build && rimraf web-build && mv build/ web-build",
    "build:server-src": "babel -d ./server-src-build ./src -s",
    "build:server": "babel -d ./server-build ./server -s",
    "build-and-push": "npm run build:web && npm run build:server-src && npm run build:server && git reset . && git add web-build server-src-build server-build -f && git ci -m \"BUILD:web-server\" && git push",
    "build:styleguide": "styleguidist build",
    "test:web-server": "curl localhost:3001",
    "watch:build-server": "fswatch -o ./server | xargs -n1 -I{} npm run build:server",
    "watch:flow": "flow:watch",
    "test:flow": "flow",
    "flow:start": "flow start",
    "flow:stop": "flow stope",
    "cra:build": "react-scripts build",
    "cra:test": "react-scripts test --env=jsdom",
    "cra:eject": "react-scripts eject",
    "deploy:setup-gh-pages": "git remote add gh-pages https://github.com/escherpad/escherpad.github.io.git",
    "gh-deploy": "git subtree push --prefix build gh-pages master",
    "build-and-deploy": "npm run build:src && git add build -f && git ci -m \"BUILD update\" && npm run gh-deploy",
    "deploy:heroku-dev": "git subtree push --prefix web-app origin heroku-web-app-dev"
  }
}
